package Math;

public class Cartesian {
	double xco;
	double yco;
	public Cartesian(double x, double y){
		xco=x;
		yco=y;
	}
	public String toString(){
		return "("+xco+","+yco+")";
	}
	public double getX(){
		return xco;
	}
	public double getY(){
		return yco;
	}
	public double getDistance(){
		return Math.sqrt(xco+yco);
	}
	

}
